#include <stdio.h>
int main() {
    long long lrgNum;                         
    scanf("%lld", &lrgNum);
    int sum = 0;
    long long temp = lrgNum;
    while (temp > 0) {
        int digit = temp % 10;
        sum += digit;
        temp /= 10;
    }
    printf("Sum of digits: %d\n", sum);
    return 0;
}
